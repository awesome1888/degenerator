{
    "name": "@generilla/cli",
    "version": "2.0.1",
    "license": "MIT",
    "main": "build.cjs/index.js",
    "module": "build.esm/index.js",
    "typings": "build.cjs/index.d.ts",
    "files": [
        "build.esm/",
        "build.cjs/",
        "generator-generator"
    ],
    "bin": {
        "generilla": "./build/index.js"
    },
    "keywords": [
        "code",
        "generator",
        "generilla",
        "cli"
    ],
    "scripts": {
        "start": "GENERILLA_DST=./../../_output GENERILLA_GENERATORS_HOME=./../../_generators ts-node src/index.ts",
        "build": "rm -rf ./build/* && yarn run build:esm && yarn run build:cjs",
        "build:cjs": "rm -rf ./build.cjs/* && tsc -p ./tsconfig.build.cjs.json && chmod +x ./build.cjs/index.js",
        "build:esm": "rm -rf ./build.esm/* && tsc -p ./tsconfig.build.esm.json",
        "build:watch": "tsc -w -p ./tsconfig.build.cjs.json",
        "lint": "eslint ./src --ext .js,.ts",
        "lint:fix": "eslint ./src --fix --ext .js,.ts",
        "release": "./script/release.sh"
    },
    "devDependencies": {
        "@types/clone-deep": "^4.0.1",
        "@types/ejs": "^2.6.3",
        "@types/fs-extra": "^8.0.1",
        "@types/inquirer": "^6.5.0",
        "@types/node": "^12.7.8",
        "nodemon": "^1.19.2",
        "ts-jest": "^24.1.0",
        "ts-node": "^8.4.1",
        "typescript": "^3.6.3"
    },
    "dependencies": {
        "@generilla/core": "^2.0.0",
        "case-formatter": "^1.0.9",
        "chalk": "^2.4.2",
        "clear": "^0.1.0",
        "clone-deep": "^4.0.1",
        "commander": "^3.0.2",
        "ejs": "^2.7.1",
        "execa": "^3.2.0",
        "figlet": "^1.2.4",
        "findit": "^2.0.0",
        "fs-extra": "^8.1.0",
        "inquirer": "^7.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
